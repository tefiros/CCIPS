<config
        xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
    <ietf-ipsec
            xmlns="http://example.net/ietf-ipsec"
            xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
        <ipsec>
            <sad>
                <sad-entry nc:operation="delete">
                    <spi>SPI_OUT</spi>
                    <!-- SPIb1=SPI2 in B-->
                    <seq-number>100</seq-number>
                    <anti-replay-window>0</anti-replay-window>
                    <rule-number>0</rule-number>
                    <local-addresses>
                        <start>LOCAL_INTERNAL</start>
                        <end>LOCAL_INTERNAL</end>
                    </local-addresses>
                    <remote-addresses>
                        <start>REMOTE_INTERNAL</start>
                        <end>REMOTE_INTERNAL</end>
                    </remote-addresses>
                    <next-layer-protocol>TCP</next-layer-protocol>
                    <local-ports>
                        <start>0</start>
                        <end>0</end>
                    </local-ports>
                    <remote-ports>
                        <start>0</start>
                        <end>0</end>
                    </remote-ports>
                    <security-protocol>esp</security-protocol>
                    <esp-sa>
                        <encryption>
                            <encryption-algorithm>ENC_ALG</encryption-algorithm>
                            <key>ENC_KEY</key>
                            <iv>VECTOR</iv>
                        </encryption>
                        <integrity>
                            <integrity-algorithm>INT_ALG</integrity-algorithm>
                            <key>INT_KEY</key>
                        </integrity>
                    </esp-sa>
                    <mode>TUNNEL</mode>
                    <tunnel>
                        <local>LOCAL_ADDRESS</local>
                        <remote>REMOTE_ADDRESS</remote>
                    </tunnel>
                    <sad-lifetime-soft>
                        <bytes>100000</bytes>
                        <packets>1000</packets>
                        <added>SOFT_TIME_ADDED</added>
                        <used>SOFT_TIME_USED</used>
                    </sad-lifetime-soft>
                    <sad-lifetime-hard>
                        <bytes>200000</bytes>
                        <packets>2000</packets>
                        <added>HARD_TIME_ADDED</added>
                        <used>HARD_TIME_USED</used>
                    </sad-lifetime-hard>
                </sad-entry>
                <sad-entry nc:operation="delete">
                    <spi>SPI_IN</spi>
                    <!-- SPIa1=SPI1 in A-->
                    <anti-replay-window>32</anti-replay-window>
                    <rule-number>0</rule-number>
                    <local-addresses>
                        <start>REMOTE_INTERNAL</start>
                        <end>REMOTE_INTERNAL</end>
                    </local-addresses>
                    <remote-addresses>
                        <start>LOCAL_INTERNAL</start>
                        <end>LOCAL_INTERNAL</end>
                    </remote-addresses>
                    <next-layer-protocol>TCP</next-layer-protocol>
                    <local-ports>
                        <start>0</start>
                        <end>0</end>
                    </local-ports>
                    <remote-ports>
                        <start>0</start>
                        <end>0</end>
                    </remote-ports>
                    <security-protocol>esp</security-protocol>
                    <esp-sa>
                        <encryption>
                            <encryption-algorithm>ENC_ALG</encryption-algorithm>
                            <key>ENC_KEY</key>
                            <iv>VECTOR</iv>
                        </encryption>
                        <integrity>
                            <integrity-algorithm>INT_ALG</integrity-algorithm>
                            <key>INT_KEY</key>
                        </integrity>
                    </esp-sa>
                    <mode>TUNNEL</mode>
                    <tunnel>
                        <local>REMOTE_ADDRESS</local>
                        <remote>LOCAL_ADDRESS</remote>
                    </tunnel>
                    <sad-lifetime-soft>
                        <bytes>100000</bytes>
                        <packets>1000</packets>
                        <added>SOFT_TIME_ADDED</added>
                        <used>SOFT_TIME_USED</used>
                    </sad-lifetime-soft>
                    <sad-lifetime-hard>
                        <bytes>200000</bytes>
                        <packets>2000</packets>
                        <added>HARD_TIME_ADDED</added>
                        <used>HARD_TIME_USED</used>
                    </sad-lifetime-hard>
                </sad-entry>
            </sad>
        </ipsec>
    </ietf-ipsec>
</config>
