<ietf-ipsec xmlns="http://example.net/ietf-ipsec" xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
<ipsec nc:operation="merge">
  <spd>
  	<spd-entry>
    		<rule-number>1</rule-number>
    		<names>
      			<name>out/192.168.201.254/192.168.202.254</name>
    		</names>
    		<condition>
      			<traffic-selector-list>
        			<ts-number>7</ts-number>
        			<direction>OUTBOUND</direction>
        			<local-addresses>
          				<start>192.168.201.0/24</start>
          				<end>192.168.201.0/24</end>
        			</local-addresses>
        			<remote-addresses>
					        <start>192.168.202.0/24</start>
					        <end>192.168.202.0/24</end>
        			</remote-addresses>
        			<next-layer-protocol>TCP</next-layer-protocol>
        			<local-ports>
          				<start>0</start>
          				<end>0</end>
        			</local-ports>
        			<remote-ports>
          				<start>0</start>
          				<end>0</end>
        			</remote-ports>
      			</traffic-selector-list>
    		</condition>
    		<processing-info>
      			<action>PROTECT</action>
      			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TUNNEL</mode>
				<tunnel>
				    <local>192.168.123.100</local>
				    <remote>192.168.123.200</remote>
				</tunnel>
			</ipsec-sa-cfg>
    		</processing-info>
  	</spd-entry>
  </spd>
  <pad>
  	<pad-entries>
      		<pad-entry-id>7</pad-entry-id>
      		<ipv4-address>192.168.123.200</ipv4-address>
      		<pad-auth-protocol>IKEv2</pad-auth-protocol>
      		<auth-method>
        		<auth-m>pre-shared</auth-m>
        		<pre-shared>
          			<secret>secreto_compartido</secret>
        		</pre-shared>
      		</auth-method>
    	</pad-entries>
  </pad>
</ipsec>
</ietf-ipsec>