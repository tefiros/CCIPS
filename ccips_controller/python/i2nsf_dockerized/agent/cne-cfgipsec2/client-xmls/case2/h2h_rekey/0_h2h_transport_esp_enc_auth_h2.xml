<ietf-ipsec xmlns="http://example.net/ietf-ipsec" xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
<ipsec>
  <spd>
	<spd-entry>
		<rule-number>10</rule-number>
		<priority>0</priority>
		<names>
			<name>in/192.168.123.100/32/192.168.123.200/32</name>
		</names>
		<condition>
			<traffic-selector-list>
				<ts-number>102</ts-number>
				<direction>INBOUND</direction>
				<local-addresses>
					<start>192.168.123.100/32</start>
 					<end>192.168.123.100/32</end>
				</local-addresses>
				<remote-addresses>
					<start>192.168.123.200/32</start>
					<end>192.168.123.200/32</end>
				</remote-addresses>
				<next-layer-protocol>TCP</next-layer-protocol>
				<local-ports>
					<start>0</start>
					<end>0</end>
				</local-ports>
				<remote-ports>
					<start>0</start>
					<end>0</end>
				</remote-ports>
			</traffic-selector-list>
		</condition>
		<processing-info>
			<action>PROTECT</action>
			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TRANSPORT</mode>
			</ipsec-sa-cfg>
		</processing-info>
	</spd-entry>
	<spd-entry>
		<rule-number>11</rule-number>
		<priority>0</priority>
		<names>
			<name>out/192.168.123.200/32/192.168.123.100/32</name>
		</names>
		<condition>
			<traffic-selector-list>
				<ts-number>2</ts-number>
				<direction>OUTBOUND</direction>
				<local-addresses>
					<start>192.168.123.200/32</start>
 					<end>192.168.123.200/32</end>
				</local-addresses>
				<remote-addresses>
					<start>192.168.123.100/32</start>
					<end>192.168.123.100/32</end>
				</remote-addresses>
				<next-layer-protocol>TCP</next-layer-protocol>
				<local-ports>
					<start>0</start>
					<end>0</end>
				</local-ports>
				<remote-ports>
					<start>0</start>
					<end>0</end>
				</remote-ports>
			</traffic-selector-list>
		</condition>
		<processing-info>
			<action>PROTECT</action>
			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TRANSPORT</mode>
			</ipsec-sa-cfg>
		</processing-info>
	</spd-entry>
	<spd-entry>
		<rule-number>12</rule-number>
		<priority>0</priority>
		<names>
			<name>fwd/192.168.123.100/32/192.168.123.200/32</name>
		</names>
		<condition>
			<traffic-selector-list>
				<ts-number>102</ts-number>
				<direction>FORWARD</direction>
				<local-addresses>
					<start>192.168.123.100/32</start>
 					<end>192.168.123.100/32</end>
				</local-addresses>
				<remote-addresses>
					<start>192.168.123.200/32</start>
					<end>192.168.123.200/32</end>
				</remote-addresses>
				<next-layer-protocol>TCP</next-layer-protocol>
				<local-ports>
					<start>0</start>
					<end>0</end>
				</local-ports>
				<remote-ports>
					<start>0</start>
					<end>0</end>
				</remote-ports>
			</traffic-selector-list>
		</condition>
		<processing-info>
			<action>PROTECT</action>
			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TRANSPORT</mode>
			</ipsec-sa-cfg>
		</processing-info>
	</spd-entry>
  </spd>
<sad>
	<sad-entry>
		<spi>201</spi> <!-- SPIb1=201 in B-->
		<rule-number>0</rule-number>
		<local-addresses>
			<start>192.168.123.100/32</start>
 			<end>192.168.123.100/32</end>
		</local-addresses>
		<remote-addresses>
			<start>192.168.123.200/32</start>
			<end>192.168.123.200/32</end>
		</remote-addresses>
		<next-layer-protocol>TCP</next-layer-protocol>
		<local-ports>
			<start>0</start>
			<end>0</end>
		</local-ports>
		<remote-ports>
			<start>0</start>
			<end>0</end>
		</remote-ports>
		<security-protocol>esp</security-protocol>
		<esp-sa>
			<encryption>
				<encryption-algorithm>3des</encryption-algorithm>
				<key>ecr_secret</key>
				<iv>vector</iv>
			</encryption>
			<integrity>
				<integrity-algorithm>hmac-md5-128</integrity-algorithm>
				<key>auth</key>
			</integrity>
		</esp-sa>
		<mode>TRANSPORT</mode>
	</sad-entry>
	<sad-entry>
		<spi>101</spi> <!-- SPIa1=101 in A -->
		<rule-number>0</rule-number>
		<local-addresses>
			<start>192.168.123.200/32</start>
 			<end>192.168.123.200/32</end>
		</local-addresses>
		<remote-addresses>
			<start>192.168.123.100/32</start>
			<end>192.168.123.100/32</end>
		</remote-addresses>
		<next-layer-protocol>TCP</next-layer-protocol>
		<local-ports>
			<start>0</start>
			<end>0</end>
		</local-ports>
		<remote-ports>
			<start>0</start>
			<end>0</end>
		</remote-ports>
		<security-protocol>esp</security-protocol>
		<esp-sa>
			<encryption>
				<encryption-algorithm>3des</encryption-algorithm>
				<key>ecr_secret</key>
				<iv>vector</iv>
			</encryption>
			<integrity>
				<integrity-algorithm>hmac-md5-128</integrity-algorithm>
				<key>auth</key>
			</integrity>
		</esp-sa>
		<mode>TRANSPORT</mode>
		<!--
		<sad-lifetime-soft>
			<bytes>3000000</bytes>
			<packets>3000</packets>
			<added>120</added>
			<used>300</used>
		</sad-lifetime-soft>
		<sad-lifetime-hard>
			<bytes>4000000</bytes>
			<packets>4000</packets>
			<added>180</added>
			<used>420</used>
		</sad-lifetime-hard>
		-->
	</sad-entry>
</sad>
</ipsec>
</ietf-ipsec>
