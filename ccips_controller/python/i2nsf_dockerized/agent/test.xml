<config
        xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
    <ietf-ipsec
            xmlns="http://example.net/ietf-ipsec"
            xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
        <ipsec>
            <spd>
                <spd-entry>
                    <rule-number>11</rule-number>
                    <priority>0</priority>
                    <names>
                        <name>in/10.0.0.101/32/10.0.0.102/32</name>
                    </names>
                    <condition>
                        <traffic-selector-list>
                            <ts-number>102</ts-number>
                            <direction>INBOUND</direction>
                            <local-addresses>
                                <start>10.0.0.101</start>
                                <end>10.0.0.101</end>
                            </local-addresses>
                            <remote-addresses>
                                <start>10.0.0.101</start>
                                <end>10.0.0.101</end>
                            </remote-addresses>
                            <next-layer-protocol>TCP</next-layer-protocol>
                            <local-ports>
                                <start>0</start>
                                <end>0</end>
                            </local-ports>
                            <remote-ports>
                                <start>0</start>
                                <end>0</end>
                            </remote-ports>
                        </traffic-selector-list>
                    </condition>
                    <processing-info>
                        <action>PROTECT</action>
                        <ipsec-sa-cfg>
                            <security-protocol>esp</security-protocol>
                            <mode>TUNNEL</mode>
                            <tunnel>
                                <local>10.0.0.101</local>
                                <remote>10.0.0.102</remote>
                            </tunnel>
                        </ipsec-sa-cfg>
                    </processing-info>
                </spd-entry>
                <spd-entry>
                    <rule-number>12</rule-number>
                    <priority>0</priority>
                    <names>
                        <name>out/10.0.0.102/32/10.0.0.101/32</name>
                    </names>
                    <condition>
                        <traffic-selector-list>
                            <ts-number>2</ts-number>
                            <direction>OUTBOUND</direction>
                            <local-addresses>
                                <start>10.0.0.101</start>
                                <end>10.0.0.101</end>
                            </local-addresses>
                            <remote-addresses>
                                <start>10.0.0.101</start>
                                <end>10.0.0.101</end>
                            </remote-addresses>
                            <next-layer-protocol>TCP</next-layer-protocol>
                            <local-ports>
                                <start>0</start>
                                <end>0</end>
                            </local-ports>
                            <remote-ports>
                                <start>0</start>
                                <end>0</end>
                            </remote-ports>
                        </traffic-selector-list>
                    </condition>
                    <processing-info>
                        <action>PROTECT</action>
                        <ipsec-sa-cfg>
                            <security-protocol>esp</security-protocol>
                            <mode>TUNNEL</mode>
                            <tunnel>
                                <local>10.0.0.102</local>
                                <remote>10.0.0.101</remote>
                            </tunnel>
                        </ipsec-sa-cfg>
                    </processing-info>
                </spd-entry>
                <spd-entry>
                    <rule-number>13</rule-number>
                    <priority>0</priority>
                    <names>
                        <name>fwd/10.0.0.101/32/10.0.0.102/32</name>
                    </names>
                    <condition>
                        <traffic-selector-list>
                            <ts-number>103</ts-number>
                            <direction>FORWARD</direction>
                            <local-addresses>
                                <start>10.0.0.101</start>
                                <end>10.0.0.101</end>
                            </local-addresses>
                            <remote-addresses>
                                <start>10.0.0.101</start>
                                <end>10.0.0.101</end>
                            </remote-addresses>
                            <next-layer-protocol>TCP</next-layer-protocol>
                            <local-ports>
                                <start>0</start>
                                <end>0</end>
                            </local-ports>
                            <remote-ports>
                                <start>0</start>
                                <end>0</end>
                            </remote-ports>
                        </traffic-selector-list>
                    </condition>
                    <processing-info>
                        <action>PROTECT</action>
                        <ipsec-sa-cfg>
                            <security-protocol>esp</security-protocol>
                            <mode>TUNNEL</mode>
                            <tunnel>
                                <local>10.0.0.101</local>
                                <remote>10.0.0.102</remote>
                            </tunnel>
                        </ipsec-sa-cfg>
                    </processing-info>
                </spd-entry>
            </spd>
            <sad>
                <sad-entry>
                    <spi>78</spi>
                    <!-- SPIb1=SPI2 in B-->
                    <seq-number>100</seq-number>
                    <anti-replay-window>0</anti-replay-window>
                    <rule-number>0</rule-number>
                    <local-addresses>
                        <start>10.0.0.0/24</start>
                        <end>10.0.0.0/24</end>
                    </local-addresses>
                    <remote-addresses>
                        <start>10.0.0.0/24</start>
                        <end>10.0.0.0/24</end>
                    </remote-addresses>
                    <next-layer-protocol>TCP</next-layer-protocol>
                    <local-ports>
                        <start>0</start>
                        <end>0</end>
                    </local-ports>
                    <remote-ports>
                        <start>0</start>
                        <end>0</end>
                    </remote-ports>
                    <security-protocol>esp</security-protocol>
                    <esp-sa>
                        <encryption>
                            <encryption-algorithm>des</encryption-algorithm>
                            <key>35e1bfdb</key>
                            <iv>35e1bfdb</iv>
                        </encryption>
                        <integrity>
                            <integrity-algorithm>hmac-sha1-160</integrity-algorithm>
                            <key>35e1bfdb</key>
                        </integrity>
                    </esp-sa>
                    <mode>TUNNEL</mode>
                    <tunnel>
                        <local>10.0.0.102</local>
                        <remote>10.0.0.101</remote>
                    </tunnel>
                    <sad-lifetime-soft>
                        <bytes>200000000</bytes>
                        <packets>4294967295</packets>
                        <added>10000</added>
                        <used>10000</used>
                    </sad-lifetime-soft>
                    <sad-lifetime-hard>
                        <bytes>4294967295</bytes>
                        <packets>4294967295</packets>
                        <added>100000</added>
                        <used>100000</used>
                    </sad-lifetime-hard>
                </sad-entry>
                <sad-entry>
                    <spi>77</spi>
                    <!-- SPIa1=SPI1 in A-->
                    <anti-replay-window>32</anti-replay-window>
                    <rule-number>0</rule-number>
                    <local-addresses>
                        <start>10.0.0.101</start>
                        <end>10.0.0.101</end>
                    </local-addresses>
                    <remote-addresses>
                        <start>10.0.0.101</start>
                        <end>10.0.0.101</end>
                    </remote-addresses>
                    <next-layer-protocol>TCP</next-layer-protocol>
                    <local-ports>
                        <start>0</start>
                        <end>0</end>
                    </local-ports>
                    <remote-ports>
                        <start>0</start>
                        <end>0</end>
                    </remote-ports>
                    <security-protocol>esp</security-protocol>
                    <esp-sa>
                        <encryption>
                            <encryption-algorithm>des</encryption-algorithm>
                            <key>35e1bfdb</key>
                            <iv>35e1bfdb</iv>
                        </encryption>
                        <integrity>
                            <integrity-algorithm>des</integrity-algorithm>
                            <key>35e1bfdb</key>
                        </integrity>
                    </esp-sa>
                    <mode>TUNNEL</mode>
                    <tunnel>
                        <local>10.0.0.101</local>
                        <remote>10.0.0.102</remote>
                    </tunnel>
                    <sad-lifetime-soft>
                        <bytes>4294967295</bytes>
                        <packets>4294967295</packets>
                        <added>10000</added>
                        <used>10000</used>
                    </sad-lifetime-soft>
                    <sad-lifetime-hard>
                        <bytes>4294967295</bytes>
                        <packets>4294967295</packets>
                        <added>100000</added>
                        <used>100000</used>
                    </sad-lifetime-hard>
                </sad-entry>
            </sad>
        </ipsec>
    </ietf-ipsec>
</config>
